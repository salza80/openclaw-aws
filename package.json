{
  "name": "@salza80/openclaw-aws",
  "version": "0.1.0",
  "description": "Deploy OpenClaw AI agents on AWS with a simple CLI",
  "keywords": [
    "openclaw",
    "moltbot",
    "clawdbot",
    "aws",
    "cdk",
    "chatbot",
    "ai",
    "whatsapp",
    "telegram",
    "discord",
    "claude",
    "infrastructure"
  ],
  "author": "Sally Mclean <smclean17@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/salza80/openclaw-aws#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/salza80/openclaw-aws.git"
  },
  "bugs": {
    "url": "https://github.com/salza80/openclaw-aws/issues"
  },
  "type": "module",
  "bin": {
    "openclaw-aws": "./dist/cli/index.js"
  },
  "files": [
    "dist/",
    "templates/",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "watch": "tsc -w",
    "dev": "tsc -w",
    "prepublishOnly": "npm run build",
    "pretest": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "pretest:coverage": "npm run build",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "format": "prettier . --write",
    "format:check": "prettier . --check"
  },
  "dependencies": {
    "@aws-sdk/client-cloudformation": "^3.978.0",
    "@aws-sdk/client-ec2": "^3.979.0",
    "@aws-sdk/client-ssm": "^3.978.0",
    "@aws-sdk/client-sts": "^3.978.0",
    "aws-cdk": "^2.237.1",
    "aws-cdk-lib": "^2.237.1",
    "chalk": "^5.6.2",
    "constructs": "^10.0.0",
    "dotenv": "^16.6.1",
    "execa": "^9.6.1",
    "ora": "^8.2.0",
    "prompts": "^2.4.2",
    "yargs": "^17.7.2"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "typescript": "~5.9.3",
    "@vitest/coverage-v8": "^3.2.0",
    "eslint": "^9.22.0",
    "eslint-config-prettier": "^10.1.0",
    "globals": "^15.15.0",
    "prettier": "^3.6.2",
    "vitest": "^3.2.0"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
